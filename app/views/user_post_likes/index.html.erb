<p id="notice"><%= notice %></p>

<h1>Listing User Post Likes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_post_likes.each do |user_post_like| %>
      <tr>
        <td><%= link_to 'Show', user_post_like %></td>
        <td><%= link_to 'Edit', edit_user_post_like_path(user_post_like) %></td>
        <td><%= link_to 'Destroy', user_post_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User post like', new_user_post_like_path %>
